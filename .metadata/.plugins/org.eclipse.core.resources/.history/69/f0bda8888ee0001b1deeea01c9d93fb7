package chapter9;

public abstract class Car {
	
	public abstract void startCar();
	public abstract void drive();
	public abstract void stop();
	public abstract void turnOff();
	
	private Car car;
	
	public Car() {
		this.car = new Avante();
	}
	
	final public void run() {			//템플릿 메서드
		startCar();
		drive();
		stop();
		turnOff();
	}
}
