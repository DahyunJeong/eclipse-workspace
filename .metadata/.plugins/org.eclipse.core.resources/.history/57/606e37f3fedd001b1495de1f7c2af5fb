package test;

import inheritance.ColorPoint;
import inheritance.Point;

public class ColorPointEx {

	public static void main(String[] args) {
		
		Point p = new Point();		//Point 클래스의 기본 생성자 호출
		
		p.set(1, 2);
		p.x = 1;
		p.y = 2;
		
		ColorPoint cp = new ColorPoint();
		
		cp.set(1, 2);
		cp.x = 3;
		cp.y = 5;
		

	}

}
