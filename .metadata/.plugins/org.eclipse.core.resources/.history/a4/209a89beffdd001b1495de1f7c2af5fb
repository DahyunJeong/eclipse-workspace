package test;

import inheritance.Point;

public class ColorPoint extends Point {
	private String color;		//점의 색 
	
	public void setColor(String color) {
		this.color = color;
//		x = 3;
//		y = 5;
//		set(1, 2);
	}
	
	public void showColorPoint() {
		System.out.println(color);
		showPoint();				//상속받은 메소드 호출
	}
	
	
}
